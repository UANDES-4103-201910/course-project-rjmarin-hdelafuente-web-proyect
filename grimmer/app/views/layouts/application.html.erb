      <form class="form-inline my-2 my-lg-0">
      </form>
    </div>
  </nav>

  <div class="container" style="margin-top: 30px">
    <div class="row">
      <div class="col-2"></div>
      <div class="col-8">
        <%= yield %>
      </div>
    </div>
  </div>
