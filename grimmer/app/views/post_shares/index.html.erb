<p id="notice"><%= notice %></p>

<h1>Post Shares</h1>

<table>
  <thead>
    <tr>
      <th>Post</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @post_shares.each do |post_share| %>
      <tr>
        <td><%= post_share.post %></td>
        <td><%= post_share.user %></td>
        <td><%= link_to 'Show', post_share %></td>
        <td><%= link_to 'Edit', edit_post_share_path(post_share) %></td>
        <td><%= link_to 'Destroy', post_share, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Post Share', new_post_share_path %>
